[
  "{{repeat(100)}}",
  {
    "companyName": "{{company()}} {{company()}}",
    "contactName": "{{firstName()}} {{surname()}}",
    "contactPhone": "{{phone()}}",
    "contactEmail": "{{email()}}",
    "description": "{{lorem(3, \"sentences\")}}",
    "address": "{{integer(100, 999)}} {{street()}}, {{city()}}, {{state()}}, {{integer(100, 10000)}}",
    "location": {
      "type": "Point",
      "coordinates": ["{{floating(-120, -80)}}", "{{floating(30, 50)}}"]
    },
    "notes": "{{lorem(2, \"sentences\")}}",
    "tags": ["{{repeat(0,7)}}", "{{lorem(1, \"words\")}}"]
  }
]
